  "dependencies": {
    "@cheqd/sdk": "^3.7.4",
    "@cheqd/ts-proto": "^3.3.7",
    "@cosmjs/proto-signing": "^0.32.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
