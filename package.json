{
	"name": "@cheqd/did-registrar",
	"version": "2.7.0",
	"description": "Universal Registrar for cheqd DID method",
	"homepage": "https://github.com/cheqd/did-registrar#readme",
	"license": "Apache-2.0",
	"author": "Cheqd Foundation Limited (https://github.com/cheqd)",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cheqd/did-registrar.git"
	},
	"type": "module",
	"bugs": {
		"url": "https://github.com/cheqd/did-registrar/issues"
	},
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./*": {
			"types": "./dist/types/*.d.ts",
			"import": "./dist/*.js",
			"default": "./dist/*.js"
		}
	},
	"scripts": {
		"dev": "nodemon src/index.ts",
		"start": "node dist/index.js",
		"build": "tsc",
		"format": "prettier --write '**/*.{js,ts,cjs,mjs,json}'",
		"test": "npx playwright test -c playwright.config.ts"
	},
	"eslintConfig": {
		"root": true,
		"extends": [
			"typescript",
			"prettier"
		]
	},
	"files": [
		"dist",
		"example.env",
		"LICENSE",
		"package.json",
		"README.md"
	],
	"dependencies": {
		"@cheqd/sdk": "^5.3.2",
		"@cheqd/ts-proto": "^4.1.0",
		"@cosmjs/proto-signing": "^0.34.0",
		"dotenv": "^16.5.0",
		"express": "^4.21.2",
		"express-validator": "^7.2.1",
		"helmet": "^8.1.0",
		"node-cache": "^5.1.2",
		"node-fetch": "^3.3.2",
		"swagger-ui-express": "^5.0.1",
		"uint8arrays": "^5.1.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.55.1",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^11.0.3",
		"@semantic-release/npm": "^12.0.1",
		"@semantic-release/release-notes-generator": "^14.0.3",
		"@stablelib/base64": "^2.0.1",
		"@types/express": "^4.17.22",
		"@types/helmet": "^4.0.0",
		"@types/node": "^24.0.15",
		"@types/node-fetch": "^2.6.12",
		"@types/swagger-ui-express": "^4.1.8",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^8.38.0",
		"@typescript-eslint/parser": "^8.38.0",
		"base64url": "^3.0.1",
		"conventional-changelog-conventionalcommits": "^9.0.0",
		"esbuild": "^0.25.5",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-config-typescript": "^3.0.0",
		"prettier": "^3.5.3",
		"semantic-release": "^24.2.5",
		"typescript": "^5.8.3"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"engines": {
		"node": ">=22.0.0"
	}
}
